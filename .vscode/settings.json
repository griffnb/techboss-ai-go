{
  "go.testEnvVars": {
    "SYS_ENV": "unit_test",
    "CONFIG_FILE": "${workspaceFolder}/.configs/unit_test.json",
    "REGION": "us-east-1"
  },
  "go.testFlags": ["-v", "-count=1", "-timeout=30s", "-race"],
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
  "--path-mode=abs",
  //"--fast-only"
],
  "go.formatTool": "custom",
  "go.alternateTools": {
    "customFormatter": "golangci-lint"
  },
  "go.formatFlags": ["fmt", "--stdin"]
}
